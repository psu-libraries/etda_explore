<!-- Google Scholar metadata -->
<meta name="citation_title" content="<%= @document[:title_tesi] %>">
<meta name="citation_author" content="<%= @document[:author_name_tesi] %>">
<meta name="citation_publication_date" content="<%= citation_publication_date %>">
<%- if citation_pdf_url %>
  <meta name="citation_pdf_url" content="<%= citation_pdf_url %>">
<%- end %>
